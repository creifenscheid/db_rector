{
	"name" : "creifenscheid/db-rector",
	"type" : "typo3-cms-extension",
	"description" : "Adapter to use typo3-rector on entries of sys_template and pages",
	"license" : "GPL-2.0-or-later",
	"require" : {
		"typo3/cms-core" : "^11.5||^12.4",
		"ssch/typo3-rector" : "^1"
	}
	,
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"autoload" : {
		"psr-4" : {
			"CReifenscheid\\DbRector\\" : "Classes/"
		}
	},
	"minimum-stability" : "dev",
	"authors" : [{
			"name" : "C. Reifenscheid",
			"email" : "dev@christian-reifenscheid.de"
		}
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "db_rector"
		}
	},
	"replace": {
		"typo3-ter/db-rector": "self.version"
	}
}
