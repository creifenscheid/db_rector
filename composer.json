{
	"name" : "creifenscheid/db-rector",
	"type" : "typo3-cms-extension",
	"description" : "This TYPO3 extension is an typo3-rector adapter with backend module to refactor sys_template.config data.",
	"license" : "GPL-2.0-or-later",
	"require" : {
		"php": "^8.0||^8.1",
		"typo3/cms-core" : "^11.5||^12.4",
		"ssch/typo3-rector" : "^1"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"autoload" : {
		"psr-4" : {
			"CReifenscheid\\DbRector\\" : "Classes/"
		}
	},
	"minimum-stability" : "alpha",
	"authors" : [{
			"name" : "Christian Reifenscheid",
			"email" : "dev@christian-reifenscheid.de"
		}
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "db_rector"
		}
	},
	"replace": {
		"typo3-ter/db-rector": "self.version"
	}
}
